<Main>:
    MDScreen:
        md_bg_color: 1, 1, 1, 1
        MDBoxLayout:
            orientation: 'vertical'
            padding: dp(5)
            size: root.width, root.height
            canvas.before:
                Color:
                    rgb: rgba(255, 210, 244, 1)
                Ellipse:
                    size: 300, 300
                    pos: -150, -210
                Ellipse:
                    size: 300, 400
                    pos: self.width-100 , (self.height/2)-50
                Ellipse:
                    size: 75, 75
                    pos: 10 , self.height-30
                Ellipse:
                    size: 100, 100
                    pos: self.width-100, (self.height/2)-150
                Color:
                    rgb: rgba(255, 113, 128, 255)
                Ellipse:
                    size: 300, 300
                    pos: -250 , (self.height/2)-150
            MDIconButton:
                icon: 'help-circle'
                pos: 10, 10
                user_font_size: '30sp'
                theme_text_color: 'Custom'
                text_color: rgba(255, 113, 128, 255)
                on_release:
                    #root.manager.transition.direction = 'left'
                    #root.manager.current = 'login'
            MDBoxLayout:
                orientation: 'vertical'
                spacing: dp(5)
                size_hint: 1, 1
                pos_hint: {"center_x": .5, "center_y": .5}
                Image:
                    source: 'logo.png'
                    size_hint: 1, 1
                    pos_hint: {"center_x": .5, "center_y": .5}
                MDLabel:
                    text: 'H e l l o !'
                    font_size: '30sp'
                    size_hint: 1, .2
                    pos_hint: {"center_x": .5, "y": .5}
                    halign: 'center'
                    color: rgba(255, 113, 128, 255)
            MDBoxLayout:
                pos_hint: {"center_x": .5, "center_y": .5}
                size_hint: 1, .3

                MDIconButton:
                    icon: 'account'
                    pos_hint: {"center_y": .5}
                    theme_text_color: 'Custom'
                MDTextField:
                    id: username_id
                    hint_text: 'u s e r n a m e'
                    font_size: 30
                    size_hint: 1, None
                    size: 100, 50
                    pos_hint: {"center_x": .5, "center_y": .5}
            MDBoxLayout:
                pos_hint: {"center_x": .5, "center_y": .5}
                size_hint: 1, .4
                MDIconButton:
                    icon: 'key'
                    pos_hint: {"center_y": .5}
                    theme_text_color: 'Custom'
                MDTextField:
                    id: password_id
                    hint_text: 'p a s s w o r d'
                    font_size: 30
                    size_hint: 1, None
                    size: 100, 50
                    pos_hint: {"center_x": .5, "center_y": .5}
                    password: True
                MDIconButton:
                    icon: "eye-off"
                    pos_hint: {"center_y": .5}
                    #pos_hint: {"center_y": .5}
                    #pos: text_field.width - self.width + dp(8), 0
                    theme_text_color: "Hint"
                    on_release:
                        self.icon = "eye" if self.icon == "eye-off" else "eye-off"
                        root.ids.password_id.password = False if root.ids.password_id.password is True else True
            MDLabel:
                id: label
                text: 'best app for showing your physics results'
                font_size: '13sp'
                size_hint: 1, .2
                pos_hint: {"center_x": .5}
                halign: 'center'
                color: rgba(127, 127, 127, 255)
            Button:
                text: 'LOGIN'
                size_hint: .5, .2
                background_color: 0, 0, 0, 0
                pos_hint: {"center_x": .5, "center_y": .1}
                on_release:
                    #root.manager.transition.direction = 'left'
                    #root.manager.current = 'login'
                    root.get_result()
                canvas.before:
                    Color:
                        rgb: rgba(111, 53, 164, 255)
                    RoundedRectangle:
                        size: self.size
                        pos: self.pos
                        radius: [5]

